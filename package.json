  },
  "homepage": "https://github.com/tomveich/mmntr#readme",
  "devDependencies": {
    "@11ty/eleventy": "^3.1.2",
    "@11ty/eleventy-img": "^6.0.4",
    "@11ty/eleventy-plugin-rss": "^2.0.4",
    "eleventy-plugin-toc": "^1.1.5",
    "markdown-it": "^14.1.0",
    "tailwindcss": "^4.1.11"
  },
  "dependencies": {
    "concurrently": "^9.2.0",
    "eleventy-plugin-date": "^1.0.0"
  }
